<Defs>
    <!--Project Maizicillin-->
    <ThingDef ParentName="ResourceBase">
		<defName>CornCultureMedium</defName>
		<label>culture medium</label>
		<description>A corn based culture medium for producing maizicillium mould.</description>
		<graphicData>
			<texPath>Things/Items/CultureMedium</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1</drawSize>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<statBases>
			<Mass>0.50</Mass>
			<Flammability>0.7</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
			<WorkToMake>700</WorkToMake>
		</statBases>
		<costList>
			<Sweetcorn>2</Sweetcorn>
			<Neutroamine>1</Neutroamine>
		</costList>
		<recipeMaker>
			<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
			<workSkill>Intellectual</workSkill>
			<recipeUsers>
				<li>TableCornProduction</li>
			</recipeUsers>
			<researchPrerequisite>Tier2CornRefining</researchPrerequisite>
			<skillRequirements>
				<Intellectual>7</Intellectual>
			</skillRequirements>
		</recipeMaker>
	</ThingDef>

    <ThingDef ParentName="ResourceBase">
		<defName>MouldyCultureMedium</defName>
		<label>maizicillium culture</label>
		<description>A successful maizicillium culture. You could extract valuable maizicillin from this.</description>
		<graphicData>
			<texPath>Things/Items/CultureMould</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>1</drawSize>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<statBases>
			<Mass>0.50</Mass>
			<Flammability>0.7</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
		</statBases>
	</ThingDef>

    <ThingDef ParentName="ResourceBase">
		<defName>Maizicillin</defName>
		<label>maizicillin</label>
		<description>Antibiotics. A poweful tool in any hospital's arsenal.</description>
		<graphicData>
			<texPath>Things/Item/Drug/Penoxycyline</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<techLevel>Industrial</techLevel>
		<statBases>
			<Mass>0.50</Mass>
			<Flammability>0.7</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
		</statBases>
	</ThingDef>

    <!--BioBuildings-->
    <ThingDef ParentName="BuildingBase">
    <defName>CultureChamber</defName>
    <label>culture chamber</label>
    <description>A chamber for cultivating nasty things on jelly.</description>
    <thingClass>Building_CultureChamber</thingClass>
    <graphicData>
      <texPath>Things/Building/Production/FermentingBarrel</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <damageData>
        <rect>(0.05,0.1,0.9,0.9)</rect>
      </damageData>
      <shadowData>
        <volume>(0.75, 0.35, 0.33)</volume>
      </shadowData>
    </graphicData>
    <minifiedDef>MinifiedThing</minifiedDef>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <fillPercent>0.45</fillPercent>
    <pathCost>60</pathCost>
    <thingCategories>
      <li>BuildingsProduction</li>
    </thingCategories>
    <statBases>
      <WorkToBuild>600</WorkToBuild>
      <Mass>10</Mass>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <costList>
      <Steel>40</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <comps>
      <li Class="CompProperties_TemperatureRuinable">
        <minSafeTemperature>20</minSafeTemperature>
        <maxSafeTemperature>32</maxSafeTemperature>
        <progressPerDegreePerTick>0.00001</progressPerDegreePerTick>
      </li>
    </comps>
    <tickerType>Rare</tickerType>
    <rotatable>true</rotatable>
    <designationCategory>Production</designationCategory>
    <constructEffect>ConstructWood</constructEffect>
    <researchPrerequisites>
      <li>Tier2CornRefining</li>
    </researchPrerequisites>
  </ThingDef>
</Defs>